{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "es2022",
    "strict": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "module": "es2022",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["jest", "node", "aws-lambda"],
    "outDir": "./build/",
    "noImplicitAny": true
  },
  "exclude": ["node_modules"]
}
